@page "~/me"
@model UserViewModel
@{
    ViewData["Title"] = "User";
    ViewData["Breadcrumb"] = Model.Breadcrumb;
}
<div class="container" style="margin-bottom: 10px;">
    <div class="card card-register mx-auto mt-5">
        <div class="card-header">Your Info</div>
        <div class="card-body">
            <form lpformnum="2" method="post" class="user-form" onchange="$('.user-form > button').prop('disabled', false);">
                <div class="form-group">
                    <div class="form-row">
                        <div class="col-md-6">
                            <div class="form-label-group">
                                <input name="firstName" value="@Model.CurrentUser.FirstName" type="text" id="firstName" class="form-control" placeholder="First name" autofocus="autofocus" style="background-image: url(&quot;data:image/png;base64,&quot;); background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; background-repeat: no-repeat no-repeat;">
                                <label for="firstName">First name</label>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-label-group">
                                <input name="lastName" value="@Model.CurrentUser.LastName" type="text" id="lastName" class="form-control" placeholder="Last name" style="background-image: url(&quot;data:image/png;base64,&quot;); background-attachment: scroll; background-size: 16px 18px; background-repeat: no-repeat no-repeat;">
                                <label for="lastName">Last name</label>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="form-label-group">
                        <input name="inputemail" value="@Model.CurrentUser.Email" disabled type="email" id="inputEmail" class="form-control" placeholder="Email address" style="background-image: url(&quot;data:image/png;base64,&quot;); background-attachment: scroll; background-size: 16px 18px; background-repeat: no-repeat no-repeat;">
                        <label for="inputEmail">Email address</label>
                    </div>
                </div>
                <div class="form-group">
                    <div class="form-row">
                        <div class="col-md-6">
                            <div class="form-label-group">
                                <input name="oldPassword" type="password" id="oldPassword" class="form-control" placeholder="Old Password" style="background-image: url(&quot;data:image/png;base64,&quot;); background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; background-repeat: no-repeat no-repeat;">
                                <label for="inputPassword">Old Password</label>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-label-group">
                                <input name="newPassword" type="password" id="newPassword" class="form-control" placeholder="New Password" style="background-image: url(&quot;data:image/png;base64,&quot;); background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; background-repeat: no-repeat no-repeat;">
                                <label for="confirmPassword">New Password</label>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="form-label-group">
                        <input name="altEmail" value="@Model.CurrentUser.UserInfo.FirstOrDefault()?.AltEmail" type="email" id="altEmail" class="form-control" placeholder="Email address (shown on resuMEs)" style="background-image: url(&quot;data:image/png;base64,&quot;); background-attachment: scroll; background-size: 16px 18px; background-repeat: no-repeat no-repeat;">
                        <label for="altEmail">Email address (shown on resuMEs)</label>
                    </div>
                </div>
                <div class="form-group">
                    <div class="form-row">
                        <div class="col-md-6">
                            <div class="form-label-group">
                                <input name="phone" value="@Model.CurrentUser.UserInfo.FirstOrDefault()?.PhoneNumber" type="text" id="phone" class="form-control" placeholder="Phone Number" style="background-image: url(&quot;data:image/png;base64,&quot;); background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; background-repeat: no-repeat no-repeat;">
                                <label for="phone">Phone Number</label>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-label-group">
                                <input name="web" value="@Model.CurrentUser.UserInfo.FirstOrDefault()?.Website" type="text" id="web" class="form-control" placeholder="Website" style="background-image: url(&quot;data:image/png;base64,&quot;); background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; background-repeat: no-repeat no-repeat;">
                                <label for="web">Website</label>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="form-label-group">
                        <input name="nameExt" value="@Model.CurrentUser.UserInfo.FirstOrDefault()?.NameExt" type="text" id="nameExt" class="form-control" placeholder="Name Extension" style="background-image: url(&quot;data:image/png;base64,&quot;); background-attachment: scroll; background-size: 16px 18px; background-repeat: no-repeat no-repeat;">
                        <label for="nameExt">Name Extension (e.g. B. Eng., or J.D.)</label>
                    </div>
                </div>
                <div class="form-group">
                    <div class="form-label-group">
                        <textarea name="summary" value="@Model.CurrentUser.UserInfo.FirstOrDefault()?.Summary" id="nameExt" class="form-control" placeholder="Professional Summary" style="background-image: url(&quot;data:image/png;base64,&quot;); background-attachment: scroll; background-size: 16px 18px; background-repeat: no-repeat no-repeat;"></textarea>
                    </div>
                </div>
                <button class="btn btn-primary btn-block" disabled>Save</button>
            </form>
        </div>
    </div>
</div>